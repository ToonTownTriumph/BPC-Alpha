        </Style>
        <Style x:Key="ToolTipDataPointStyle" TargetType="charting:LineDataPoint">
            <Style.Resources>
                <SolidColorBrush x:Key="brushtest" Color="{Binding HomeBrush}"/>
            </Style.Resources>
            <Setter Property="Background" Value="#0077CC" />
            <Setter Property="BorderBrush" Value="White"/>
            <Setter Property="BorderThickness" Value="2"/>
            <Setter Property="IsTabStop" Value="False"/>
            
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="charting:LineDataPoint">
                        <ControlTemplate.Resources>
                            <SolidColorBrush x:Key="SCBTest" Color="{Binding HomeBrush}"/>
                        </ControlTemplate.Resources>
                        <Grid x:Name="Root" Opacity="1">
